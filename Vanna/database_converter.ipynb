    "import sqlite3\n",
    "import psycopg2\n",
    "\n",
    "# SQLite connection\n",
    "sqlite_conn = sqlite3.connect(\"eceDay.db\")\n",
    "sqlite_cursor = sqlite_conn.cursor()\n",
    "\n",
    "# PostgreSQL connection\n",
    "pg_conn = psycopg2.connect(\n",
    "    host=\"34.60.28.103\",\n",
    "    dbname=\"ece_day_db\",\n",
    "    user=\"postgres\", \n",
    "    password=\"butlar\"\n",
    ")\n",
    "pg_cursor = pg_conn.cursor()\n",
    "\n",
    "# Get all table names from SQLite\n",
    "sqlite_cursor.execute(\"SELECT name FROM sqlite_master WHERE type='table';\")\n",
    "tables = sqlite_cursor.fetchall()\n",
    "\n",
    "for table_name in tables:\n",
    "    table_name = table_name[0]\n",
    "    \n",
    "    # Skip internal SQLite table\n",
    "    if table_name == \"sqlite_sequence\":\n",
    "        print(f\"Skipping internal table: {table_name}\")\n",
    "        continue\n",
    "\n",
    "    # Get CREATE TABLE SQL\n",
    "    sqlite_cursor.execute(f\"SELECT sql FROM sqlite_master WHERE type='table' AND name='{table_name}'\")\n",
    "    create_stmt = sqlite_cursor.fetchone()[0]\n",
    "\n",
    "    # Fix schema conversion (safe replacement)\n",
    "    create_stmt_pg = (\n",
    "        create_stmt.replace(\"AUTOINCREMENT\", \"\")  # PostgreSQL SERIAL is auto-incrementing\n",
    "                   .replace(\"INTEGER PRIMARY KEY\", \"SERIAL PRIMARY KEY\")\n",
    "                   .replace(\"TEXT\", \"VARCHAR\")\n",
    "                   .replace(\"DATETIME\", \"TIMESTAMP\")\n",
    "    )\n",
    "\n",
    "    try:\n",
    "        pg_cursor.execute(f\"DROP TABLE IF EXISTS {table_name} CASCADE\")\n",
    "        pg_cursor.execute(create_stmt_pg)\n",
    "        print(f\"Created table: {table_name}\")\n",
    "    except Exception as e:\n",
    "        print(f\"‚ùå Error creating table {table_name}: {e}\")\n",
    "        pg_conn.rollback()  # Important: Reset transaction state\n",
    "        continue\n",
    "\n",
    "    # Get data from SQLite\n",
    "    sqlite_cursor.execute(f\"SELECT * FROM {table_name}\")\n",
    "    rows = sqlite_cursor.fetchall()\n",
    "\n",
    "    # Get column names\n",
    "    sqlite_cursor.execute(f\"PRAGMA table_info({table_name})\")\n",
    "    col_names = [col[1] for col in sqlite_cursor.fetchall()]\n",
    "    col_names_str = \", \".join(col_names)\n",
    "    placeholders = \", \".join([\"%s\"] * len(col_names))\n",
    "\n",
    "    # Insert data into PostgreSQL\n",
    "    for row in rows:\n",
    "        try:\n",
    "            pg_cursor.execute(\n",
    "                f\"INSERT INTO {table_name} ({col_names_str}) VALUES ({placeholders})\",\n",
    "                row\n",
    "            )\n",
    "        except Exception as e:\n",
    "            print(f\"Error inserting row into {table_name}: {e}\")\n",
    "\n",
    "pg_conn.commit()\n",
    "sqlite_conn.close()\n",
    "pg_cursor.close()\n",
    "pg_conn.close()"
   ]
  }
